## Ignore Visual Studio temporary files, build results, and
## files generated by popular Visual Studio add-ons.

# Build results
bin/
obj/
out/

# User-specific files
*.user
*.userosscache
*.suo
*.sln.docstates

# Visual Studio cache/options directory
.vs/

# Generated SQLite / LocalDb
*.db
*.db-shm
*.db-wal
*.mdf
*.ldf

# Logs
logs/
*.log

# Secrets
appsettings.json

# VS Code
.vscode/

# JetBrains Rider
.idea/

# Publish outputs
publish/
dist/
